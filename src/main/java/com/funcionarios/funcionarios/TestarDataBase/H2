CREATE TABLE IF NOT EXISTS LISTAFUNCIONARIO
(

    id    SERIAL PRIMARY KEY NOT NULL,
    NOME  VARCHAR(50) NOT NULL,
    EMAIL VARCHAR(50) NOT NULL,
    CPF   VARCHAR(50) NOT NULL UNIQUE

);

INSERT INTO LISTAFUNCIONARIO ( CPF, EMAIL, NOME) VALUES ('123.456.789-01', 'WILL@HOTMAIL.COM', 'WILLIAN MARIANO');
INSERT INTO LISTAFUNCIONARIO ( CPF, EMAIL, NOME) VALUES ('123.456.789-02', 'MARIA@HOTMAIL.COM', 'MARIA DO BAIRRO');
